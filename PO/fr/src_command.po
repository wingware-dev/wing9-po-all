#: src/command/argentry.py:152
msgid "Cannot execute %s: incorrect value for %s"
msgstr "Impossible d'exécuter %s : valeur incorrecte pour %s"

#: src/command/commandmgr.py:154
msgid "%sarginfo[%s].doc not valid.  Using None."
msgstr "%sarginfo[%s].doc n'est pas valide. None est utilisé."

#: src/command/commandmgr.py:161
msgid "%sarginfo[%s].type not valid.  Treating arg as small string."
msgstr ""
"%sarginfo[%s].type n'est pas valide. Les arguments sont traités commes des "
"chaînes courtes."

#: src/command/commandmgr.py:168
msgid "%sarginfo[%s].formlet not valid.  Treating arg as small string."
msgstr ""
"%sarginfo[%s].formlet n'est pas valide. Les arguments sont traités commes "
"des chaînes courtes."

#: src/command/commandmgr.py:201
msgid "Callable %s.arginfo[%s].%s failed.  Using default."
msgstr ""
"L'appelable %s.arginfo[%s].%s a échoué. Utilisation de la configuration par "
"défaut."

#: src/command/commandmgr.py:458
msgid "Command name is not a string"
msgstr "Le nom de la commande n'est pas une chaîne"

#: src/command/commandmgr.py:461
msgid "Command doc is not a None, string, or callable"
msgstr "Le doc de la commande n'est ni None, ni une chaîne, ni appelable"

#: src/command/commandmgr.py:465
msgid "Command label is not a string or callable"
msgstr "Le label de la commande n'est ni une chaîne, ni appelable"

#: src/command/commandmgr.py:541
msgid "%sarginfo not dict or callable.  Using short string args."
msgstr ""
"%arginfo n'est ni un dictionnaire, ni appelable. Utilisation des arguments "
"en chaîne courte."

#: src/command/commandmgr.py:553
msgid "%sarginfo[%s]: %s not an arg name."
msgstr "%sarginfo[%s] : %s n'est pas un nom d'argument."

#: src/command/commandmgr.py:562
msgid "%sarginfo[%s] not CArgInfo or callable"
msgstr "%sarginfo[%s] n'est ni un CArgInfo, ni appelable"

#: src/command/commandmgr.py:571
msgid "%sarginfo[%s]: exception inspecting value"
msgstr "%sarginfo[%s] : exception durant l'inspection de la valeur"

#: src/command/commandmgr.py:583
msgid "%sarginfo for %s not found.  Using short string."
msgstr "%sarginfo pour %s non trouvé. Utilisation d'une chaîne courte."

#: src/command/commandmgr.py:817
msgid "Error executing %s.available()"
msgstr "Erreur à l'exécution de %s.available()"

#: src/command/commandmgr.py:843 src/command/commandmgr.py:858
msgid "Tried to execute unavailable command %s"
msgstr "Tentative d'exécution de la commande non disponible %s"

#: src/command/commandmgr.py:875
msgid "Error obtaining args for script %s"
msgstr "Erreur en obtenant les arguments pour le script %s"

#: src/command/commandmgr.py:878
msgid "Error obtaining args for command %s"
msgstr "Erreur en obtenant les arguments pour la commande %s"

#: src/command/commandmgr.py:915
msgid "Error executing script %s"
msgstr "Erreur durant l'exécution du script %s"

#: src/command/commandmgr.py:918
msgid "Error executing command %s"
msgstr "Erreur durant l'exécution de la commande %s"

#: src/command/commandmgr.py:991
msgid "Callable %s.arginfo raised an exception"
msgstr "L'appel %s.arginfo a levé une exception"

#: src/command/commandmgr.py:998
msgid ""
"Callable %s.arginfo returned invalid arginfo; treating args as short string"
msgstr ""
"L'appel %s.arginfo a retourné des arginfo invalides ; traitant des arguments "
"commes des chaînes courtes"

#: src/command/commandmgr.py:1017
msgid "Default "
msgstr "Défaut "

#: src/command/commandmgr.py:1020
msgid "%s: Callable default arginfo failed"
msgstr "%s : L'appel à l'arginfo par défaut a échoué"

#: src/command/commandmgr.py:1038
msgid "Callable %s.arginfo[%s] failed; treating arg as short string"
msgstr ""
"L'appel %s.arginfo[%s] a échoué ; traitement des arguments commes des "
"chaînes courtes"

#: src/command/commandmgr.py:1068
msgid "Callable %s.label failed; using label derived from command name"
msgstr ""
"L'appel à %s.label a échoué ; utilisant un label dérivé du nom de commande"

#: src/command/commandmgr.py:1073
msgid ""
"Callable %s.label returned non-string; using label derived from command name"
msgstr ""
"L'appel à %s.label a retourné autre chose qu'une chaîne ; utilisant un label "
"dérivé du nom de commande"

#: src/command/commandmgr.py:1103
msgid "Callable %s.doc failed; using ''"
msgstr "L'appel à %s.doc a échoué ; utilisant ''"

#: src/command/commandmgr.py:1108
msgid "Callable %s.doc returned non-string; using ''"
msgstr "L'appel à %s.doc a retourné autre chose qu'une chaîne ; utilisant ''"

#: src/command/commandmgr.py:1135
msgid "Callable %s.flags returned non-dict; using {}"
msgstr ""
"L'appel au drapeaux %s n'a pas retourné un dictionnaire; utilisation de {}."

#: src/command/commandmgr.py:1478
msgid "* Error(s) loading %s.%s.%s:"
msgstr "* Erreur(s) en chargeant %s.%s.%s :"

#: src/command/commandmgr.py:1487
msgid "Exception while loading %s.%s.%s"
msgstr "Déclenchement d'une exception durant le chargement de %s.%s.%s"

